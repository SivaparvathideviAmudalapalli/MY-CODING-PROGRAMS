#include<stdio.h>
#include<string.h>
int main()
{
	char s[20];
	int i,x=0,cnt=0,y=0;
	gets(s);
	for(i=0;s[i]!='\0';i++)
	{
		if(isupper(s[i]))
		x=x+1;
		else if(islower(s[i]))
		cnt=ctn+1;
		else if(isdigit(s[i]))
		y=y+1;
	}
	printf("\ncount of uppercase letters:%d",x);
	printf("\ncount of lowercase letters:%d",cnt);
	printf("\ncount of digits :%d",y);
}
